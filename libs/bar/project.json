{
  "name": "bar",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/bar/src",
  "prefix": "monoleasa-is-single",
  "tags": [],
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/angular:package",
      "outputs": ["{workspaceRoot}/dist/{projectRoot}"],
      "options": {
        "project": "libs/bar/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/bar/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/bar/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/bar/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/bar/**/*.ts", "libs/bar/**/*.html"]
      }
    },
    "publish": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cd ./dist/libs/bar && npm pack && npm publish"
      }
    }
  }
}
